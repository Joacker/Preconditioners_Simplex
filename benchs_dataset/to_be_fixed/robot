variables

x(1) in [-inf, inf]
x(2) in [-inf, inf]
x(3) in [-inf, inf]
x(4) in [-inf, inf]
x(5) in [-inf, inf]
x(6) in [-inf, inf]
x(7) in [-inf, inf]

minimize ((((((x(1)^2+x(2)^2)+x(3)^2)+x(4)^2)+x(5)^2)+x(6)^2)+x(7)^2)

constraints

(((((((cos(x(1))+cos(x(2)))+cos(x(3)))+cos(x(4)))+cos(x(5)))+cos(x(6)))+(0.5*cos(x(7))))-4)=0

(((((((sin(x(1))+sin(x(2)))+sin(x(3)))+sin(x(4)))+sin(x(5)))+sin(x(6)))+(0.5*sin(x(7))))-4)=0

