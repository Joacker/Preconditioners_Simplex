variables

x1 in [-oo, oo];
x2 in [-oo, oo];
x3 in [-oo, oo];
x4 in [-oo, oo];
x5 in [-oo, oo];
x6 in [-oo, oo];
x7 in [-oo, oo];

minimize ((((((x1^2+x2^2)+x3^2)+x4^2)+x5^2)+x6^2)+x7^2)

constraints

(((((((cos(x1)+cos(x2))+cos(x3))+cos(x4))+cos(x5))+cos(x6))+(0.5*cos(x7)))-4)=0;

(((((((sin(x1)+sin(x2))+sin(x3))+sin(x4))+sin(x5))+sin(x6))+(0.5*sin(x7)))-4)=0;

end